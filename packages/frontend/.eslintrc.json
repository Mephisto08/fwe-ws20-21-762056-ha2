/*
* In dieser Datei werden die Konfigurationen f√ºr den eslint festgelegt
*/
{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "eslint-plugin-prettier"
    ],
    "extends": [
      "google",
      "eslint-config-prettier"
    ],
    "rules":{
      "prettier/prettier": ["error",
        {
          "semi": true,
          "trailingComma": "all",
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 2,
          "useTabs": true
        }
      ],
      "linebreak-style": 0
    }
  }
  